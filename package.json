{
  "name": "leila-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:firebase": "npm run build && firebase deploy --only hosting",
    "images": "tsx scripts/generate-service-images.ts",
    "images:generate": "tsx scripts/generate-service-images.ts generate",
    "images:optimize": "tsx scripts/generate-service-images.ts optimize",
    "images:component": "tsx scripts/generate-service-images.ts component",
    "ai:generate": "tsx scripts/generate-ai-images.ts",
    "ai:check": "tsx -e \"import { checkAvailableServices } from './scripts/ai-image-generator.js'; checkAvailableServices();\"",
    "pro:images": "tsx scripts/generate-pro-images.ts",
    "google:check": "tsx -e \"import { checkGoogleSetup } from './scripts/google-image-generator.js'; checkGoogleSetup();\"",
    "google:generate": "tsx -e \"import { generateAllGoogleImages } from './scripts/google-image-generator.js'; generateAllGoogleImages();\"",
    "google:simple": "tsx scripts/google-imagen-simple.ts",
    "google:imagefx": "tsx scripts/google-imagen-simple.ts imagefx",
    "process:watch": "node scripts/process-downloaded-images.js watch",
    "process:batch": "node scripts/process-downloaded-images.js batch",
    "imagen2:check": "tsx -e \"import { checkImagen2Setup } from './scripts/google-imagen2-premium.js'; checkImagen2Setup();\"",
    "imagen2:generate": "tsx -e \"import { generateGooglePremiumImages } from './scripts/google-imagen2-premium.js'; generateGooglePremiumImages();\"",
    "imagen2:single": "tsx -e \"const [c,s] = process.argv.slice(2); import('./scripts/google-imagen2-premium.js').then(m => m.generateSpecificImagen2(c,s));\"",
    "imagen2:equipment": "tsx -e \"import { generateGoogleImagesNoPeople } from './scripts/google-imagen2-no-people.js'; generateGoogleImagesNoPeople();\"",
    "ai-designer": "tsx scripts/ai-graphic-designer.ts",
    "generate-asset": "tsx scripts/generate-asset.ts",
    "generate-service-images": "tsx scripts/ai-graphic-designer.ts batch -f scripts/services-list.txt -t serviceCard",
    "generate-all-services": "tsx scripts/generate-all-service-images.ts"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.11.1",
    "@google-cloud/functions": "^4.1.0",
    "@google-cloud/pubsub": "^5.1.0",
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "@types/axios": "^0.9.36",
    "@types/node-fetch": "^2.6.12",
    "@types/qrcode.react": "^1.0.5",
    "@upstash/redis": "^1.35.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^1.1.1",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "firebase": "^11.9.1",
    "firebase-admin": "^13.4.0",
    "framer-motion": "^12.18.1",
    "geolib": "^3.3.4",
    "google-auth-library": "^10.1.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.517.0",
    "next": "15.3.4",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.15.3",
    "sharp": "^0.34.2",
    "sonner": "^2.0.5",
    "stripe": "^18.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "typescript": "^5"
  }
}
